{
  "name": "slim",
  "version": "1.0.0",
  "description": "Simracing League Incident Manager",
  "main": "index.js",
  "scripts": {
    "prepare": "husky install",
    "lint": "eslint --ext .ts . --max-warnings 0",
    "test": "mocha",
    "migrate:make": "node -r ts-node/register scripts/migrate.ts create",
    "migrate:latest": "node -r ts-node/register scripts/migrate.ts latest",
    "migrate:down": "node -r ts-node/register scripts/migrate.ts down"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "fastify": "4.18.0",
    "kysely": "0.26.1",
    "neverthrow": "6.0.0",
    "pg": "8.11.2"
  },
  "devDependencies": {
    "@jarsec/eslint-config": "3.2.1",
    "@jarsec/prettier-config": "1.2.1",
    "@jarsec/tsconfig": "1.0.0",
    "@types/chai": "4.3.6",
    "@types/mocha": "10.0.1",
    "@types/node": "20.5.1",
    "@types/pg": "8.10.2",
    "@typescript-eslint/parser": "6.4.0",
    "chai": "4.3.8",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.53.0",
    "husky": "8.0.3",
    "kysely-migration-cli": "0.2.1",
    "lint-staged": "14.0.0",
    "mocha": "10.2.0",
    "prettier": "3.0.2",
    "ts-node": "10.9.1",
    "typescript": "5.2.2"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "ava": {
    "extensions": {
      "ts": "module"
    },
    "nodeArguments": [
      "--loader=ts-node/esm"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.ts": "eslint --fix"
  }
}
